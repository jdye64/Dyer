<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <bean id="powerService" class="com.jeremydyer.service.power.impl.PowerServiceImpl">
        <property name="networkServicePort" value="${service.gpio.power.port}" />
    </bean>

    <bean id="photoService" class="com.jeremydyer.service.media.impl.PhotoServiceImpl">
        <property name="fileSystemBasePath" value="${photo.filesystem.base.path}" />
    </bean>

</beans>